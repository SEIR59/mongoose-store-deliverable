{% layout "layout.liquid" %} {% block content %}
<div>
  <a href="/products/new"><button>Add Product</button></a>
  {% for product in products %}
  <article>
      <div class="item-div">
        <h3>
          {{ product.name }}
        </h3>
      <a href="/products/{{ product._id }}">
        <img src="{{ product.img }}" />
      </a>
        <br />
        {{ product.qty }} in Stock!
        <br />
        Price: ${{ product.price }}
      </div>
  </article>
  {% endfor %} {% endblock %}
</div>
